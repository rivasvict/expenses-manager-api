{
    "extends": "airbnb-base",
    "ignorePatterns": ["node_modules/", "data/"],
    "rules" : {"comma-dangle": ["error", "never"], "prefer-arrow-callback": "off", "func-names": "off", "no-underscore-dangle": "off", "object-curly-newline": "off"},
    "plugins": ["jest"],
    "env": {
      "jest/globals": true,
      "jest" : true,
      "mocha" : true,
      "node" : true
    }
}
